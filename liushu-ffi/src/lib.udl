namespace liushu {

};

[Error]
enum LiushuError {
    "Other"
};

enum CandidateSource {
    "Hmm", "CodeTable"
};

dictionary Candidate {
    string text;
    string code;
    string? comment;
    u64 weight;
    CandidateSource source;
};

interface Engine {
    [Throws=LiushuError]
    constructor([ByRef] string dict_path);

    [Throws=LiushuError]
    sequence<Candidate> search([ByRef] string code);
};
